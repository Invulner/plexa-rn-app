{
  "expo": {
    "name": "Plexa",
    "description": "Plexa AI",
    "slug": "Plexa",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": ["ios", "android"],
    "version": "1.0.2",
    "orientation": "portrait",
    "icon": "src/assets/images/icon.png",
    "splash": {
      "image": "src/assets/images/nav-bg_final-01.png",
      "resizeMode": "cover",
      "backgroundColor": "#ededed"
    },
    "updates": {
      "fallbackToCacheTimeout": 0,
      "enabled": true
    },
    "assetBundlePatterns": [
      "src/assets/images/*"
    ],
    "ios": {
      "bundleIdentifier": "com.plexa.native",
      "buildNumber": "2",
      "icon": "src/assets/icons/iTunesArtwork@2x.png",
      "config": {
        "usesNonExemptEncryption": false
      },
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "Please provide the Plexa app with permission to access your photos."
      }
    }
  }
}
